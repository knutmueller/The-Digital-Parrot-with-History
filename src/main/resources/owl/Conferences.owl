        <rdfs:label xml:lang="en">Workshop Session</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Session"/>
    </Class>

    <ObjectProperty rdf:about="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#chairs">
        <rdfs:comment xml:lang="en">The act of chairing a session or conference.</rdfs:comment>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:label xml:lang="en">chairs</rdfs:label>
        <rdfs:range rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
            <unionOf rdf:parseType="Resource">
                <rdf:first rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Session"/>
                <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Conference"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:rest>
            </unionOf>
        </rdfs:range>
        <inverseOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#hasChair"/>
    </ObjectProperty>

    <ObjectProperty rdf:about="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#hasChair">
        <rdfs:comment xml:lang="en">To link to the chair of a session or conference.</rdfs:comment>
        <rdfs:domain rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
            <unionOf rdf:parseType="Resource">
