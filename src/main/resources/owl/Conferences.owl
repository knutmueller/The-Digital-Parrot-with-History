    <Class rdf:about="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Tutorial">
        <rdfs:comment xml:lang="en">A tutorial.</rdfs:comment>
        <rdfs:label xml:lang="en">Tutorial</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Presentation"/>
    </Class>

    <Class rdf:about="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#TutorialSession">
        <rdfs:comment xml:lang="en">A session with tutorials.</rdfs:comment>
        <rdfs:label xml:lang="en">Tutorial Session</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Session"/>
        <rdfs:subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
            <allValuesFrom rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Tutorial"/>
            <onProperty rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#hasPresentation"/>
        </rdfs:subClassOf>
