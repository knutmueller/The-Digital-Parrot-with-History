        <disjointWith rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#SocialEvent"/>
        <disjointWith rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Track"/>
    </Class>

    <Class rdf:about="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#PresentationSession">
        <rdfs:comment xml:lang="en">A session with talks, as opposed to a demo session or a poster session.</rdfs:comment>
        <rdfs:label xml:lang="en">Presentation Session</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Session"/>
        <rdfs:subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
            <allValuesFrom rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Talk"/>
            <onProperty rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#hasPresentation"/>
        </rdfs:subClassOf>
        <disjointWith rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#DemoSession"/>
        <disjointWith rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#PosterSession"/>
    </Class>

    <Class rdf:about="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Session">
        <rdfs:comment xml:lang="en">A session. Most conferences come divided into sessions.</rdfs:comment>
