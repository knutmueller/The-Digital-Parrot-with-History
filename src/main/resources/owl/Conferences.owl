        <rdfs:label xml:lang="en">Poster Presentation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Presentation"/>
    </Class>

    <Class rdf:about="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#PosterSession">
        <rdfs:comment xml:lang="en">A poster session. Note that this has instances of PosterPresentation as its hasPresentation instances -- the Poster is used to describe the actual poster that is being shown.</rdfs:comment>
        <rdfs:label xml:lang="en">Poster Session</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Session"/>
        <rdfs:subClassOf rdf:parseType="Resource">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
            <allValuesFrom rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#PosterPresentation"/>
            <onProperty rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#hasPresentation"/>
        </rdfs:subClassOf>
        <disjointWith rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#DemoSession"/>
        <disjointWith rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#PresentationSession"/>
    </Class>

    <Class rdf:about="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/Conferences/2008/11/Conferences.owl#Presentation">
        <rdfs:comment xml:lang="en">Something that happens in a session. Note that a typical paper presentation is a Talk, not a Presentation.</rdfs:comment>
        <rdfs:label xml:lang="en">Presentation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/TimeAndPlace/2008/11/TimeAndPlace.owl#PlacedThing"/>
        <rdfs:subClassOf rdf:resource="http://parrot.resnet.scms.waikato.ac.nz/Parrot/Terms/TimeAndPlace/2008/11/TimeAndPlace.owl#TimedThing"/>
